<script>

setInterval(clo,1000);
function clo()
{
  var dx=new Date();
var dt=document.getElementById("clock").innerHTML=dx.toDateString();
}


var slideIndex = 0;
showSlides();

function showSlides() {
  var i;
  var slides = document.getElementsByClassName("mySlides");
  var dots = document.getElementsByClassName("dot");
  for (i = 0; i < slides.length; i++) {
    slides[i].style.display = "none";  
  }
  slideIndex++;
  if (slideIndex > slides.length) {slideIndex = 1}    
  for (i = 0; i < dots.length; i++) {
    dots[i].className = dots[i].className.replace(" active", "");
  }
  slides[slideIndex-1].style.display = "block";  
  dots[slideIndex-1].className += " active";
  setTimeout(showSlides, 2000); // Change image every 2 seconds
}


var slideInde = 0;
showSlid();

function showSlid() {
  var i;
  var slid = document.getElementsByClassName("mySlid");
  var dt = document.getElementsByClassName("dt");
  for (i = 0; i < slid.length; i++) {
    slid[i].style.display = "none";  
  }
  slideInde++;
  if (slideInde > slid.length) {slideInde = 1}    
  for (i = 0; i < dt.length; i++) {
    dt[i].className = dt[i].className.replace(" active", "");
  }
  slid[slideInde-1].style.display = "block";  
  dt[slideInde-1].className += " active";
  setTimeout(showSlid, 2000); // Change image every 2 seconds
}




</script>

